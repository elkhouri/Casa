<%= render 'parent_form' if signed_in? %>
  <div class='page-header'>
    <h3><%= @user.name %>'s Authorized Pickups</h3>
  </div>
  <% if @children.any? %>
    <div class='row'>
      <%= render @children %>
    </div>
    <%= will_paginate @children%>
  <% end %>

<script>
  $('[rel=popover]').popover({placement:'bottom', html:true, trigger:'hover'})
  $('[name=signin_button]').click(function(){
    alert('You\'ve Signed In!')
  })
  $('[name=signout_button]').click(function(){
    alert('You\'ve Signed Out!')
  })
</script>